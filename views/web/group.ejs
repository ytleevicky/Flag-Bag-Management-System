<body>

    <br>

    <h2><a href="/management">活動管理</a> > <a href="/viewitem">First Event</a> > 團體義工</h2>
  
    <br>
  
    <div class="bordered">
  
      <p class="font-weight-bold" style="background-color:powderblue; padding: 10px; ">匯入及匯出</p>
  
      <div class="row" style="padding: 20px;">
  
        <form action="/import_event" method="post" enctype="multipart/form-data">
          <div class="col-sm-6">
            <p>匯入Excel檔案：</p>
            <input type="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" name="file" multiple />
            <input type="submit" />
          </div>
        </form>
  
        <div class="col-sm-6">
          <p>以Excel檔案匯出：</p>
          <a href="/export_group" class="btn btn-info">匯出</a>
        </div>
  
      </div>
  
  
    </div>
  
    <br>
  
    <br>
    <a href="/addGroup" class="btn btn-primary">建立</a>
    <button type="button" class="btn btn-danger">删除</button>
    <br>
  
    <table class="table table-bordered">
      <br>
      <thead>
        <tr>
          <th scope="col">
            <div class="form-check">
              <input type="checkbox" class="form-check-input" id="exampleCheck1">
              <label class="form-check-label" for="exampleCheck1"></label>
            </div>
          </th>
            <th scope="col">團體姓名</th>
            <th scope="col">旗站位置</th>
            <th scope="col">賣旗地區</th>
            <th scope="col">義工人數</th>
            <th scope="col">編輯</th>
          </tr>
      </thead>
      <tbody>

        <% webs.forEach( function(model) { %>
          <tr>
            <th scope="row">
              <div class="form-check">
                <input type="checkbox" class="form-check-input" id="exampleCheck1">
                <label class="form-check-label" for="exampleCheck1"></label>
              </div>
            </th>
              <td><%= model.vGroupName %></td>
              <td><%= model.sLocation %></td>
              <td><%= model.location %></td>
              <td><%= model.numOfV %></td>
              <td><a href='' class="btn btn-primary">編輯</a>
                </td>
            <% }); %>
          </tr>
      
        </tbody>
      </table>

